<!-- Hero Section-->
<section style="background: url(image-path('listing-hero-bg.jpg')) no-repeat;" class="hero listing-single-hero d-flex align-items-end">
  <div class="container">
    <div class="content d-flex justify-content-between align-items-start flex-column flex-lg-row align-items-lg-end">
    </div>
  </div>
</section>
<!-- Hero Section-->
<section class="hero hero-inverse">
  <div class="container">
    <ul class="breadcrumb">
      <li class="breadcrumb-item"><a href="index.html">Home</a></li>
      <li class="breadcrumb-item active">New York</li>
    </ul>
    <h1>New York</h1>
    <div class="subcategories">
      <h3>City Quarters            </h3>
      <ul class="nav nav-pills-template">
        <li class="nav-item"><a href="#" class="nav-link active">Mount Vernon</a></li>
        <li class="nav-item"><a href="#" class="nav-link">Johnstown</a></li>
        <li class="nav-item"><a href="#" class="nav-link">Jamestown</a></li>
        <li class="nav-item"><a href="#" class="nav-link">Hudson</a></li>
        <li class="nav-item"><a href="#" class="nav-link">Kingston</a></li>
      </ul>
    </div>
  </div>
</section>
<div class="container">
  <div class="row">
    <!-- Votes     -->
    <main class="col-lg-12">
      <div class="widget opening-hours">
        <header>
          <h3 class="has-lines"><small>Resultats</small><%=@location%> </h3>
        </header>
        <table class="table">
          <thead>
            <tr>
              <th>#</th>
              <th>Canditats</th>
              <th>Vote physique</th>
              <th>Vote reseaux sociaux</th>
              <th>Vote total</th>
              <th>Pourcentage</th>
            </tr>
          </thead>
          <tbody>
            <% @votes.each_with_index do |vote, index| %>
              <tr>
                <th scope="row"><%="#{index + 1}"%></th>
                <td><%= vote[:candidate].candidate_name %></td>
                <td><%= vote[:offline] %></td>
                <td><%= vote[:online] %></td>
                <td><%= vote[:total] %></td>
                <td><%= "#{vote[:percentage]} %" %></td>
              </tr>
            <%end%>
          </tbody>
        </table>
      </div>
    </main>
  </div>
</div>
